{
  "opts": {
    "template": "./node_modules/postman-jsdoc-theme",
    "encoding": "utf8",
    "destination": "./out/",
    "recurse": true,
    "readme" : "readme.md"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include" : [],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": false
    }
  }
}
